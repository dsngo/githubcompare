{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ {
      "source" : "index_bundle.js",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=3600"
      } ]
    } ]
  }
}
